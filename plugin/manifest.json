{
    "name": "Plugins List",
    "description": "Export list of plugins as text",
    "author": "griffin-stewie",
    "homepage": "https://github.com/griffin-stewie/PluginsList",
    "version": "0.1.12",
    "identifier": "net.cyan-stivy.sketch-plugin.PluginsList",
    "icon": "icon.png",
    "appcast": "https://raw.githubusercontent.com/griffin-stewie/PluginsList/master/.appcast.xml",
    "compatibleVersion": 3,
    "bundleVersion": 1,
    "commands": [
        {
            "name": "Handle Actions",
            "identifier": "handle-actions",
            "script": "index.js",
            "handlers": {
                "actions": {
                    "Startup": "onStartup",
                    "Shutdown": "onShutdown"
                }
            }
        },
        {
            "name": "Export as...",
            "identifier": "export_as",
            "script": "index.js",
            "handler": "exportPluginsList",
            "shortcut": "cmd ctrl e",
            "description": "Exprot List of Plugins"
        }
    ],
    "menu": {
        "items": [
            "export_as"
        ]
    }
}