{
  "$schema": "https://raw.githubusercontent.com/BohemianCoding/SketchAPI/develop/docs/sketch-plugin-manifest-schema.json",
  "icon": "icon.png",
  "commands": [
    {
      "name": "Export as CSV",
      "identifier": "net.cyan-stivy.sketchPlugin.export-as-csv",
      "handler": "exportAsCSV",
      "script": "__main.js"
    },
    {
      "name": "Export as Markdown Table",
      "identifier": "net.cyan-stivy.sketchPlugin.export-as-markdown-table",
      "handler": "exportAsMarkdownTable",
      "script": "__main.js"
    },
    {
      "name": "Export as Backlog Table",
      "identifier": "net.cyan-stivy.sketchPlugin.export-as-backlog-table",
      "handler": "exportAsBacklogTable",
      "script": "__main.js"
    }
  ],
  "menu": {
    "title": "Plugin List",
    "items": [
      "net.cyan-stivy.sketchPlugin.export-as-csv",
      "net.cyan-stivy.sketchPlugin.export-as-markdown-table",
      "net.cyan-stivy.sketchPlugin.export-as-backlog-table"
    ]
  },
  "version": "0.2.0",
  "description": "Sketch Plugin that gives you list of plugins you enabled as several text formats.",
  "name": "Plugin List",
  "identifier": "Plugin List",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com/griffin-stewie/PluginsList/master/.appcast.xml",
  "author": "griffin-stewie"
